<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox id="StatusBar" alignment="CENTER_LEFT" maxHeight="20.0" minHeight="18.0" prefHeight="20.0" spacing="5.0"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.statusBar.StatusBar">
    <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
    </padding>
    <Label fx:id="baudRateLabel" alignment="BOTTOM_LEFT" maxHeight="1.7976931348623157E308" maxWidth="100.0"
           minWidth="100.0" prefHeight="16.0" prefWidth="100.0" text="Baud rate:" HBox.hgrow="ALWAYS">
        <font>
            <Font size="11.0" fx:id="x3"/>
        </font>
        <textFill>
            <Color blue="0.625" green="0.625" red="0.625" fx:id="x4"/>
        </textFill>
    </Label>
    <Label text="Port:" textFill="#9f9f9f">
        <font>
          <Font size="11.0"/>
        </font>
    </Label>
    <Label fx:id="chosenPortLabel" maxWidth="40.0" minWidth="35.0" prefWidth="35.0" textFill="#9f9f9f">
        <font>
          <Font size="11.0"/>
        </font>
    </Label>
    <Label fx:id="statePortLabel" maxWidth="40.0" minWidth="40.0" prefWidth="40.0" textFill="#9f9f9f">
        <font>
          <Font size="11.0"/>
        </font>
    </Label>
    <Pane HBox.hgrow="ALWAYS"/>
    <Label font="$x3" text="Right status" textFill="$x4" HBox.hgrow="NEVER"/>
</HBox>
